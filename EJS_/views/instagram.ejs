<%- include("include_/intro.ejs"); %>

<body>
    <h1>Username is : <%= data.name %></h1>
    <button>Follow</button>
    <button>Message</button>
    <hr>
    <% for(let post of data.posts) { %>
        <img src="<%= post.image %>" alt="Some image"> 
        <b>Likes : <%= post.likes%></b> &nbsp; &nbsp;&nbsp;&nbsp; <b>Comments : <%=post.comments%></b>

    <% } %>
    <%- include("include_/footer.ejs"); %>
    <script src="/script.js"></script>
</body>
</html>